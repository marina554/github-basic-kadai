      "source": [
        "# Humanクラスの定義\n",
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(\"名前:\", self.name)\n",
        "        print(\"年齢:\", self.age)\n",
        "\n",
        "\n",
        "# インスタンスを作成して変数に代入\n",
        "person = Human(\"太郎\", 25)\n",
        "\n",
        "# メソッドを呼び出す\n",
        "person.printinfo()\n"
      ]
    }
